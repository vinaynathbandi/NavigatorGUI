<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Navigator">
  <test name="Testing WorkGroup server viewlet options">
  <parameter name="sDriver" value="webdriver.chrome.driver"/>
  <parameter name="sDriverpath" value="Drivers/chromedriver.exe"/>
  <parameter name="URL" value="/navigator/#/login"/>
  <parameter name="uname" value="Admin"/>
  <parameter name="password" value="admin"/>
  
  <parameter name="ConnectionInstanceName" value="REMOTE_QMGRS"/>
  <parameter name="RemoteQueueManagerName" value="QMGR_NAME"/>
  <parameter name="ConnectionName" value="localhost"/>
  <parameter name="CommandQueueName" value="SYSTEM.ADMIN.COMMAND.QUEUE"/>
  <parameter name="ChannelName" value="SYSTEM.DEF.SVRCONN"/>
  
  <parameter name="NewConnectionName" value="192.168.1.210"/>
 
  
   <classes>
    <class name="ApodGUI.AddRemoteQueueManager19564">
     
     
      <methods>
       <include name="Login"/>
       <include name="AddRemoteQueueManager"/>
       <include name="Logout"/>
      </methods>
      
    </class>
   </classes>
   
   </test>
  </suite> 