<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Navigator">
 <test name="Test Manager Viewlet">
    <parameter name="sDriver" value="webdriver.chrome.driver"/>
    <parameter name="sDriverpath" value="Drivers/chromedriver.exe"/>
    <parameter name="URL" value="/navigator/#/login"/>
    <parameter name="uname" value="Admin"/>
    <parameter name="password" value="admin"/>
    
    <parameter name="Dashboardname" value="ManagerOptions19787"/>
    
    <parameter name="wgs" value="WGS10 - 1"/>
    <parameter name="Managername" value="Manager"/>
    
    <parameter name="Start" value="Start all WMQ objects"/>
    <parameter name="Stop" value="Stop all WMQ objects"/>
    <parameter name="commands" value="Commands"/>
    <parameter name="ManagerOptions" value="Show Object Attributes,Show Topology,Create Queue Manager,Commands,Properties...,MQSC,Discover now,Delete,Delete from database,Events...,Add to favorites..."/>
      
     <classes>
      <class name="ApodGUI.ManagerOptions19787">
       
       <methods>
       
        <include name="Login"/>
        <include name="ManagerOptions"/> 
        <include name="Logout"/> 
        
       </methods>
       
       </class>
     </classes>
 </test>
 
 </suite>